{
    "compilerOptions": {
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2015.promise"
        ],
        "module": "amd",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "src/gis/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}